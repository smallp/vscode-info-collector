{
	"name": "small-collector",
	"description": "Language server in node for small-collector.",
	"version": "0.1.0",
	"author": "small",
	"license": "MIT",
	"engines": {
		"node": "*"
	},
	"dependencies": {
		"vscode-languageserver": "^3.0.4"
	},
	"devDependencies": {
		"@types/node": "^6.0.52",
		"typescript": "^2.1.5"
	},
	"scripts": {
		"compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
		"watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc --watch -p ."
	}
}